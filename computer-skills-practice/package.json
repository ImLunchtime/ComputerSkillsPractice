{
  "name": "computer-skills-practice",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "dev:host": "vite --host",
    "build": "vite build",
    "preview": "vite preview",
    "preview:host": "vite preview --host",
    "server": "nodemon server/app.js",
    "server:prod": "node server/app.js",
    "start": "concurrently \"npm run server\" \"npm run dev\"",
    "start-public": "concurrently \"npm run server\" \"npm run dev:host\"",
    "start:prod": "npm run build && concurrently \"npm run server:prod\" \"npm run preview:host\""
  },
  "dependencies": {
    "@iconify-prerendered/vue-material-symbols": "^0.28.1755063979",
    "@tailwindcss/vite": "^4.1.14",
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^12.4.1",
    "concurrently": "^9.2.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "tailwindcss": "^4.1.14",
    "uuid": "^13.0.0",
    "vue": "^3.5.22",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.1",
    "nodemon": "^3.1.10",
    "vite": "^7.1.7",
    "vite-plugin-vue-devtools": "^8.0.2"
  }
}
